    Set objFSO = CreateObject("Scripting.FileSystemObject")
    Set objFile = objFSO.OpenTextFile("C:\Automation\EmailTargets_Test.txt", ForReading)
    Set WshShell = WScript.CreateObject("WScript.Shell")
    Const ForReading = 1
    Dim strEmail
    
    Do Until objFile.AtEndOfStream
    strEmail = objFile.ReadLine


      '######### Fire Up the youtube Login URL incognito to punch the creds from the CSV ##########

           siteURL = "https://accounts.google.com/signin/v2/identifier?continue=https%3A%2F%2Fwww.youtube.com%2Fsignin%3Fapp%3Ddesktop%26action_handle_signin%3Dtrue%26next%3D%252Faccount%26hl%3Den%26feature%3Dredirect_login&hl=en&passive=true&service=youtube&uilel=3&flowName=GlifWebSignIn&flowEntry=ServiceLogin"
           Set browobj = CreateObject("Wscript.Shell")
           browobj.Run "chrome -incognito -url "&siteURL
           Set browobj = Nothing
           wscript.sleep (6000)

      '######### Username #############

           wscript.sleep (6000)
           Set WshShell = WScript.CreateObject("WScript.Shell")
           wscript.sleep (6000) 
           wshshell.sendkeys strEmail
           wscript.sleep (6000)
           WshShell.SendKeys "{ENTER}" 
           wscript.sleep (1500)

      '######### Password #############

           wscript.sleep (6000)
           Set WshShell = WScript.CreateObject("WScript.Shell")  
           Wshshell.SendKeys "hardtoguess@123"
           wscript.sleep (6000)
           WshShell.SendKeys "{ENTER}" 
           wscript.sleep (1500)

     
      '########## Now Go for My Youtube Job ############     
   
           Set WshShell = WScript.CreateObject("WScript.Shell")
           WshShell.SendKeys "^{t}"
           WshShell.SendKeys "{ENTER}"
           wscript.sleep (6000)
           WshShell.SendKeys "<your youtube video link>"
         
